    pod 'Alamofire', '~> 4.4.0'
    pod 'PusherSwift'

  target 'MapTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MapUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
